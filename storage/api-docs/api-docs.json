    "paths": {
        "/api/auth/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Register new user",
                "operationId": "d764dd091cc4494ae0baf360b03319f3",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "name",
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "name": {
                                        "type": "string",
                                        "example": "Andri Sul"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "me@andrisul.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "mY_s3creT"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Login user",
                "operationId": "8dcb70df1020986038d098cc08d05dae",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "me@andrisul.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "mY_s3creT"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/auth/logout": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Logout user",
                "operationId": "69281b12abb272c76871f19cb17ca563",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/sale": {
            "post": {
                "tags": [
                    "Sales"
                ],
                "summary": "Save Sale",
                "operationId": "d64da8ed23875861493edb24176dc753",
                "requestBody": {
                    "description": "Payload Example: `{'vehicle_id': '60ad2383822394f3dd463', 'sale_date': '2023-05-14', 'sale_price': 170000000}`",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "vehicle_id",
                                    "sale_date",
                                    "sale_price"
                                ],
                                "properties": {
                                    "vehicle_id": {
                                        "type": "string",
                                        "example": "60ad2383822394f3dd463"
                                    },
                                    "sale_date": {
                                        "type": "string",
                                        "example": "2023-05-14"
                                    },
                                    "sale_price": {
                                        "type": "number",
                                        "example": "170000000"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/sales": {
            "get": {
                "tags": [
                    "Sales"
                ],
                "summary": "Get paginated list of sales",
                "operationId": "a144e6f88c308ff943433956e711b0a3",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page number",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    },
                    {
                        "name": "perPage",
                        "in": "query",
                        "description": "Items per page",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 10
                        }
                    },
                    {
                        "name": "searchQuery",
                        "in": "query",
                        "description": "Search query",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "brio"
                        }
                    },
                    {
                        "name": "filters",
                        "in": "query",
                        "description": "Additional filters",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "type:car"
                        }
                    },
                    {
                        "name": "sortField",
                        "in": "query",
                        "description": "Sort field",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "created_at"
                        }
                    },
                    {
                        "name": "sortDirection",
                        "in": "query",
                        "description": "Sort direction",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "desc"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/vehicle": {
            "post": {
                "tags": [
                    "Vehicles"
                ],
                "summary": "Save vehicle",
                "operationId": "870f3d1d125d24e1fcca3190cad58f82",
                "requestBody": {
                    "description": "Example for Car:\n     *         `{'type': 'car', 'name': 'Honda Brio', 'release_year': 2015, 'color': 'white', 'price': 170000000, 'engine': 'engine2', 'passenger_capacity': 4, 'car_type': 'carType1'}`\n     *         Example for Motorcycle:\n     *         `{'type': 'motorcycle', 'name': 'Yamaha Nmax 155', 'release_year': 2023, 'color': 'red', 'price': 35000000, 'engine': 'engine1', 'suspension_type': 'suspension1', 'transmission_type': 'automatic'}`",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "type",
                                    "name",
                                    "release_year",
                                    "color",
                                    "price",
                                    "engine"
                                ],
                                "properties": {
                                    "type": {
                                        "type": "string",
                                        "enum": [
                                            "car",
                                            "motorcycle"
                                        ],
                                        "example": "motorbike"
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Yamaha Nmax 155"
                                    },
                                    "release_year": {
                                        "type": "number",
                                        "example": "2023"
                                    },
                                    "color": {
                                        "type": "string",
                                        "example": "red"
                                    },
                                    "price": {
                                        "type": "number",
                                        "example": "35000000"
                                    },
                                    "engine": {
                                        "type": "string",
                                        "example": "engine1"
                                    },
                                    "suspension_type": {
                                        "type": "string",
                                        "example": "suspension1"
                                    },
                                    "transmission_type": {
                                        "type": "string",
                                        "example": "automatic"
                                    },
                                    "passenger_capacity": {
                                        "type": "number",
                                        "example": "4"
                                    },
                                    "car_type": {
                                        "type": "string",
                                        "example": "carType1"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
